{
  "cli": {
    "version": ">= 15.0.14",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "env": {
        "EXPO_TOKEN": "your-expo-token-here" // замените на реальный токен или удалите
      }
    },
    "preview": {
      "distribution": "internal",
      "env": {
        "EXPO_TOKEN": "your-expo-token-here" // замените на реальный токен или удалите
      }
    },
    "production": {
      "autoIncrement": true,
      "distribution": "store",
      "env": {
        "EXPO_TOKEN": "your-expo-token-here" // замените на реальный токен или удалите
      }
    }
  }
}
